  "scripts": {
    "prepare": "tsc -p tsconfig-client.json && tsc -p tsconfig-server.json && tsc -p tsconfig-unlogged.json && mixin-patch && npx webpack",
    "build-ignore-error": "(tsc -p tsconfig-client.json || echo \"continue w/error\") && (tsc -p tsconfig-server.json  || echo \"continue w/error\") && (tsc -p tsconfig-unlogged.json || echo \"continue w/error\") && mixin-patch && npx webpack",
    "build-cli": "(tsc -p tsconfig-client.json || echo \"continue w/error\") && (tsc -p tsconfig-unlogged.json || echo \"continue w/error\") && mixin-patch  && npx webpack",
    "watch:buildC": "tsc -p tsconfig-client.json --watch",
    "watch:buildS": "tsc -p tsconfig-server.json --watch",
    "watch:buildU": "tsc -p tsconfig-unlogged.json --watch",
    "test": "tsc -p tsconfig-test.json && mocha --reporter spec --bail --check-leaks dist/test/test/",
    "test-directo": "mocha --reporter spec --bail --check-leaks dist/test/test/",
    "dump": "npm start -- --dump-db",
    "dumpb": "(tsc -p tsconfig-server.json  || echo \"continue w/error\") && npm run dump",
    "start": "node dist/server/server/server-principal.js"
  }
